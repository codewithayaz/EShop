@model IEnumerable<PageLink>
<ol class="breadcrumb float-sm-right">
    @foreach (var item in Model)
    {
        if (Model.Last() == item)
        {
            <li class="breadcrumb-item active">@item.Name</li>
        }
        else
        {
            <li class="breadcrumb-item"><a href="@item.URLPath"> @item.Name</a></li>
        }
    }
</ol>